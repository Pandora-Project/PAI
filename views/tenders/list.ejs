<%- include('../layout') %>
<main>
  <h1><%= title %></h1>
  <table>
    <thead>
      <tr>
        <th>#</th>
        <th>Name</th>
        <th>Institution</th>
        <th>Start Date</th>
        <th>End Date</th>
        <th>Details</th>
      </tr>
    </thead>
    <tbody>
      <% tenders.forEach((tender, index) => { %>
        <tr>
          <td><%= index + 1 %></td>
          <td><%= tender.name %></td>
          <td><%= tender.institution %></td>
          <td><%= tender.startDate.toLocaleString() %></td>
          <td><%= tender.endDate.toLocaleString() %></td>
          <td><a href="/tenders/<%= tender.id %>">View Details</a></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</main>
